{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT", ".env*", "**/src/*"],
      "outputs": [".svelte-kit/**"]
    },
    "lint": {
      "cache": false
    },
    "fix": {
      "cache": false
    },
    "check": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
